{
  "manifest_version": 3,
  "name": "Uber Cookies Finder",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "clipboardWrite"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
